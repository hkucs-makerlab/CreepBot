{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "catkin_make",
			"args": [
				"-DCMAKE_BUILD_TYPE=RelWithDebInfo"
			],
			"problemMatcher": [
				"$catkin-gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "catkin_make: build"
		},
		{
			"type": "catkin_make",
			"args": [
				"clean"
			],
			"problemMatcher": [
				"$catkin-gcc"
			],
			"group": {
				"kind": "none",
				"isDefault": false
			},
			"label": "catkin_make: clean"
		}
	]
}